= form_for @admin_feedback do |f|
  - if @admin_feedback.errors.any?
    #error_explanation
      %h2= "#{pluralize(@admin_feedback.errors.count, "error")} prohibited this admin_feedback from being saved:"
      %ul
        - @admin_feedback.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :content
    = f.text_field :content
  .field
    = f.label :user_id
    = f.number_field :user_id
  .actions
    = f.submit 'Save'
