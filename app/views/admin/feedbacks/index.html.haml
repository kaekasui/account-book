%h1 Listing admin_feedbacks

%table
  %tr
    %th Name
    %th User
    %th
    %th
    %th

  - @admin_feedbacks.each do |admin_feedback|
    %tr
      %td= admin_feedback.content
      %td= admin_feedback.user_id
      %td= link_to 'Show', ['admin', admin_feedback]
      %td= link_to 'Edit', edit_admin_feedback_path(admin_feedback)
      %td= link_to 'Destroy', ['admin', admin_feedback], :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New Feedback', new_admin_feedback_path
